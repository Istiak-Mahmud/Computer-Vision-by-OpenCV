   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "pic = cv2.imread('image.jpg',0)\n",
    "cols=pic.shape[1]\n",
    "rows=pic.shape[0]\n",
    "M=np.float32([[1,0,150],[0,1,70]])\n",
    "shifted=cv2.warpAffine(pic,M,(cols,rows))\n",
    "#img = cv2.imwrite('image.png',img)\n",
    "cv2.imshow('shifted',shifted)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
